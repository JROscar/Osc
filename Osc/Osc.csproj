<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <AssemblyName>Osc.Net.WebApi</AssemblyName>
        <RootNamespace>Osc.Net.WebApi</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0"/>
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Osc.Net.Model\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Osc.Net.Model\**" />
    </ItemGroup>

    <ItemGroup>
      <Content Remove="Osc.Net.Model\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Osc.Net.Model\**" />
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="Osc.Net.Model\obj\Osc.Net.Model.csproj.nuget.dgspec.json" />
      <_ContentIncludedByDefault Remove="Osc.Net.Model\obj\project.assets.json" />
      <_ContentIncludedByDefault Remove="Osc.Net.Model\obj\project.packagespec.json" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="Osc.Net.Model\Osc.Net.Model.csproj" />
    </ItemGroup>

</Project>
